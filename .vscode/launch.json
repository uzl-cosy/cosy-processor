  "version": "0.2.0",
  "configurations": [
    {
      "name": "cosy",
      "type": "debugpy",
      "request": "launch",
      "module": "cosy",
      "python": "${workspaceFolder}/.venv/bin/python",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "args": ["-c", "config/cosy.yml"]
    },
    {
      "name": "cosy_local",
      "type": "debugpy",
      "request": "launch",
      "module": "cosy",
      "python": "${workspaceFolder}/.venv/bin/python",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "args": ["-c", "config/cosy.yml", "config/local.yml"]
    }
  ]
}
